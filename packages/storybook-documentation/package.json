{
  "name": "@komune-io/g2-storybook-documentation",
  "version": "0.5.0-alpha.1",
  "description": "Components for storybook documentation",
  "author": "Komune",
  "license": "Apache-2.0",
  "private": false,
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "type": "module",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "publish:gitlab": "npm publish",
    "publish:npm": "yarn publish --access public --new-version ${VERSION}"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/react": "^18.0.28"
  },
  "peerDependencies": {
    "@emotion/css": "^11.10.6",
    "@mui/material": "^5.11.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@komune-io/g2-themes": "0.5.0-alpha.1",
    "@komune-io/g2-utils": "0.5.0-alpha.1",
    "@storybook/addon-docs": "^7.2.1"
  },
  "publishConfig": {
    "@komune-io:registry": "https://npm.pkg.github.com"
  }
}
