{
  "name": "@komune-io/g2-utils",
  "repository": "https://github.com/komune-io/fixers-g2.git",
  "author": "Komune",
  "license": "Apache-2.0",
  "version": "0.5.0-alpha.1",
  "description": "Utility package used in the g2 packages.",
  "private": false,
  "main": "dist/index.cjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "publish:github": "npm publish",
    "publish:npm": "yarn publish --access public --new-version ${VERSION}"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/react": "^18.0.28"
  },
  "peerDependencies": {
    "@emotion/css": "^11.11.2",
    "@mui/material": "^5.15.14",
    "notistack": "3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@mantine/hooks": "^7.7.0",
    "@tanstack/react-query": "^5.28.6",
    "i18next": "^23.10.1",
    "lodash-es": "^4.17.21"
  },
  "publishConfig": {
    "@komune-io:registry": "https://npm.pkg.github.com"
  }
}
